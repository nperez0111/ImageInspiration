 <div class="col-md-6 noFloat center">
     <div class="input-group">
      <input type="search" class="form-control" placeholder="Search for..." value="{{query}}">
      <span class="input-group-btn">
        <button class="btn btn-primary" on-click="search">Search</button>
      </span>
    </div>
</div>

<div class="result">
	{{#each images:i}}
		<Image ctx="{{images[i]}}" titles="{{titles[i]}}" />
	{{/each}}
	
</div>